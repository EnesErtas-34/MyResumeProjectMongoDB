@model Education
@{
    ViewData["Title"] = "CreateEducation";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<div class="container">
    <form method="post">
        <label>Başlık</label>
        <input type="text" asp-for="Title" class="form-control" />
        <br />
        <label>Okul</label>
        <input type="text" asp-for="School" class="form-control" />
        <br />
        <label>Tarih</label>
        <input type="text" asp-for="Time" class="form-control" />
        <br />
        <button class="btn btn-outline-success">Ekle</button>
    </form>
</div>

